{
    "extends:": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["es6"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "exclude": [
      "node_modules",
      "babel.config.json",
      "metro.config.js",
      "jest.config.js"
    ]
}